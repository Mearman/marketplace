{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "hooks": {},
  "permissions": {
    "deny": [
      "Edit(eslint.config.ts)",
      "Edit(**/eslint.config.ts)",
      "Write(eslint.config.ts)",
      "Write(**/eslint.config.ts)",
      "Bash(git add -A*)",
      "Bash(git add --all*)",
      "Bash(git add . *)",
      "Bash(git add . &&*)",
      "Bash(*&& git add -A*)",
      "Bash(*&& git add --all*)",
      "Bash(*&& git add .*)",
      "Bash(git commit -a*)",
      "Bash(git commit --all*)",
      "Bash(*&& git commit -a*)",
      "Bash(*&& git commit --all*)",
      "Bash(git commit --no-verify*)",
      "Bash(*--no-verify*)",
      "Bash(git push --no-verify*)",
      "Bash(*&& git commit --no-verify*)",
      "Bash(*&& git push --no-verify*)"
    ]
  }
}
